{
	"name": "cocoon-cloud-sdk",
	"version": "2.0.2",
	"description": "Cocoon.io is a cloud service that allows any web developer to build performant mobile native apps/games based on their HTML5 content.",
	"keywords": [
		"apps",
		"cli",
		"cloud",
		"cocoon",
		"cocoon.io",
		"compiler",
		"html5",
		"games",
		"gulp",
		"grunt",
		"javascript",
		"js",
		"sdk",
		"typescript"
	],
	"author": "Ludei Developers <devs@ludei.com>",
	"license": "MIT",
	"homepage": "https://github.com/CocoonIO/cocoon-cloud-sdk",
	"repository": {
		"type": "git",
		"url": "https://github.com/CocoonIO/cocoon-cloud-sdk"
	},
	"bugs": {
		"url": "https://github.com/CocoonIO/cocoon-cloud-sdk/issues"
	},
	"files": [
		"examples",
		"declaration",
		"out"
	],
	"main": "out/index.js",
	"types": "declarations/index.d.ts",
	"directories": {
		"example": "./examples",
		"lib": "./src",
		"test": "./spec"
	},
	"tonicExampleFilename": "examples/runkit.js",
	"scripts": {
		"docs": "typedoc --name 'Cocoon Cloud SDK' --out docs --json docs/project.json --excludePrivate --ignoreCompilerErrors && cp -f assets/* docs/",
		"inspect-spec": "eslint spec/tests && jshint spec/tests",
		"inspect-src": "tslint --project tsconfig.json",
		"postinstall": "npm run transpile",
		"test": "jasmine",
		"transpile": "tsc"
	},
	"dependencies": {
		"@types/node": "^9.3.0",
		"cocoon-xml-sugar": "^2.0.0",
		"detect-node": "^2.0.3",
		"popsicle": "^9.2.0",
		"popsicle-status": "^2.0.1"
	},
	"devDependencies": {
		"@types/form-data": "^2.2.1",
		"@types/tough-cookie": "^2.3.2",
		"eslint": "^4.15.0",
		"jasmine": "^2.8.0",
		"jshint": "^2.9.5",
		"tslint": "^5.8.0",
		"typedoc": "^0.9.0",
		"typescript": "^2.6.2"
	}
}
