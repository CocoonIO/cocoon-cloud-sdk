{
  "name": "cocoon-cloud-sdk",
  "version": "2.0.1",
  "description": "Cocoon.io is a cloud service that allows any web developer to build performant mobile native apps/games based on their HTML5 content.",
	"keywords": [
		"apps",
		"cli",
		"cloud",
		"cocoon",
		"cocoon.io",
		"compiler",
		"html5",
		"games",
		"gulp",
		"grunt",
		"javascript",
		"js",
		"sdk",
		"typescript"
	],


	"author": "Ludei Developers <devs@ludei.com>",
	"license": "MIT",
	"homepage": "https://github.com/CocoonIO/cocoon-cloud-sdk",
	"repository": {
		"type": "git",
		"url": "https://github.com/CocoonIO/cocoon-cloud-sdk"
	},
	"bugs": {
		"url": "https://github.com/CocoonIO/cocoon-cloud-sdk/issues"
	},


	"files": [
    "examples",
    "declaration",
    "out"
  ],
	"main": "out/index.js",
	"types": "declarations/index.d.ts",
	"directories": {
		"example": "./examples",
		"lib": "./src",
		"test": "./spec"
	},
	"tonicExampleFilename": "examples/runkit.js",


	"scripts": {
    "docs": "typedoc --name 'Cocoon Cloud SDK' --out docs --json docs/project.json --excludePrivate --ignoreCompilerErrors && cp -f assets/* docs/",
    "inspect-spec": "eslint spec/tests && jshint spec/tests; exit 0",
    "inspect-src": "tslint --project tsconfig.json; exit 0",
    "postinstall": "npm run transpile; exit 0",
    "test": "jasmine",
    "transpile": "tsc"
  },
	"dependencies": {
    "@types/node": "^7.0.14",
    "cocoon-xml-sugar": "^2.0.0",
    "detect-node": "^2.0.3",
    "popsicle": "^9.1.0",
    "popsicle-status": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "jasmine": "^2.5.3",
    "jshint": "^2.9.4",
    "tslint": "^5.0.0",
    "typedoc": "^0.6.0",
    "typescript": "^2.2.2"
  }
}
