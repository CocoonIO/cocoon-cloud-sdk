{
  "name": "cocoon-cloud-sdk",
  "version": "2.0.1",
  "description": "Cocoon.io is a cloud service that allows any web developer to build performant mobile native apps/games based on their HTML5 content.",
  "files": [
    "out",
	  "declaration"
  ],
  "main": "out/index.js",
  "types": "declaration/index.d.ts",
  "scripts": {
    "docs": "node_modules/.bin/typedoc --name 'Cocoon Cloud SDK' --out docs --json docs/project.json --excludePrivate --ignoreCompilerErrors && cp -f assets/* docs/",
    "inspect-spec": "node_modules/.bin/eslint spec/tests && node_modules/.bin/jshint spec/tests; exit 0",
    "inspect-src": "node_modules/.bin/tslint --project tsconfig.json; exit 0",
    "postinstall": "node_modules/.bin/tsc; exit 0",
    "test": "node_modules/.bin/jasmine",
    "transpile": "node_modules/.bin/tsc; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CocoonIO/cocoon-cloud-sdk"
  },
  "keywords": [
    "apps",
    "cli",
    "cloud",
    "cocoon",
    "cocoon.io",
    "compiler",
    "html5",
    "games",
    "gulp",
    "grunt",
    "javascript",
    "js",
    "sdk",
    "typescript"
  ],
  "author": "Ludei Developers <devs@ludei.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CocoonIO/cocoon-cloud-sdk/issues"
  },
  "homepage": "https://github.com/CocoonIO/cocoon-cloud-sdk",
  "dependencies": {
    "cocoon-xml-sugar": "^2.0.0",
    "detect-node": "^2.0.3",
    "popsicle": "^9.1.0",
    "popsicle-status": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^7.0.12",
    "eslint": "^3.19.0",
    "jasmine": "^2.5.3",
    "jshint": "^2.9.4",
    "tslint": "^5.0.0",
    "typedoc": "^0.5.10",
    "typescript": "^2.2.2"
  }
}
